    "classnames": "^2.1.2",
    "prop-types": "^15.6.0",
    "provide-array": "^1.0.0",
    "provide-map": "^1.0.0",
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "react-redux-provide": "^6.0.0-0 || ^7.0.0-0",
