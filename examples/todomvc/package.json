    "raw-loader": "^0.5.1",
    "react-redux-provide-test-utils": "^1.0.15",
    "react-transform-hmr": "^1.0.0",
    "style-loader": "^0.12.3",
    "todomvc-app-css": "^2.0.1",
    "webpack": "^1.9.11",
